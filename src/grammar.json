{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "ctrmml",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_item"
      }
    },
    "_item": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "meta_command"
        },
        {
          "type": "SYMBOL",
          "name": "at_command"
        },
        {
          "type": "SYMBOL",
          "name": "instrument_type"
        },
        {
          "type": "SYMBOL",
          "name": "note"
        },
        {
          "type": "SYMBOL",
          "name": "rest"
        },
        {
          "type": "SYMBOL",
          "name": "command_with_number"
        },
        {
          "type": "SYMBOL",
          "name": "command"
        },
        {
          "type": "SYMBOL",
          "name": "escape_command"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "track_selector"
        },
        {
          "type": "SYMBOL",
          "name": "operator"
        },
        {
          "type": "SYMBOL",
          "name": "punctuation"
        },
        {
          "type": "SYMBOL",
          "name": "param_key"
        }
      ]
    },
    "meta_command": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "meta_keyword"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "meta_platform_value"
                },
                {
                  "type": "SYMBOL",
                  "name": "meta_value"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "meta_keyword": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "#(?:title|composer|author|date|comment|platform|option|game|composerj|programmer|[A-Za-z][A-Za-z0-9_-]*)"
      }
    },
    "meta_platform_value": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\s+(?:megadrive|mdsdrv)[^\\n]*"
      }
    },
    "meta_value": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\s+[^\\n]*"
      }
    },
    "at_command": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "@(?:[A-Za-z]\\d+|\\d+)"
      }
    },
    "instrument_type": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "(?:fm|psg|pcm|2op)"
      }
    },
    "note": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "(?:[a-gh](?:[+\\-=])?)+"
      }
    },
    "rest": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "r"
      }
    },
    "command_with_number": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "(?:o|l|q|Q|C|R|s|t|T|v|V|p|k|K|E|M|P|G|D|_+)[+-]?\\d+(?:\\.\\d+)?"
      }
    },
    "command": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "(?:o|l|q|s|t|v|p|k|__|_|\\^|&)"
      }
    },
    "escape_command": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\\\=?"
      }
    },
    "number": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[+-]?\\d+(?:\\.\\d+)?"
      }
    },
    "string": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\"(?:[^\"\\\\]|\\\\.)*\"|'(?:[^'\\\\]|\\\\.)*'"
      }
    },
    "track_selector": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "(?:\\*[0-9]+|[A-Z]+)"
      }
    },
    "operator": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[+\\-*\\/=<>:()]"
      }
    },
    "punctuation": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[\\[\\]\\{\\}\\|.,]"
      }
    },
    "param_key": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "(?:rate|offset)"
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": ";[^\\n]*"
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}